<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.tsj.ui.news.NewsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.tsj.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_contacts"
        android:name="com.example.tsj.ui.contact.ContactFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_contacts" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.example.tsj.ui.history.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history">
    </fragment>

    <fragment
        android:id="@+id/navigation_balance"
        android:name="com.example.tsj.ui.balance.BalanceFragment"
        android:label="fragment_balance"
        tools:layout="@layout/fragment_balance" />

    <fragment
        android:id="@+id/navigation_application"
        android:name="com.example.tsj.ui.application.ApplicationFragment"
        android:label="fragment_application"
        tools:layout="@layout/fragment_application" />
    <fragment
        android:id="@+id/personalFragment"
        android:name="com.example.tsj.ui.personal.PersonalFragment"
        android:label="fragment_personal"
        tools:layout="@layout/fragment_personal" />
    <fragment
        android:id="@+id/navigation_balance_detail"
        android:name="com.example.tsj.ui.balanceDetail.BalanceDetailFragment"
        android:label="fragment_balance_detail"
        tools:layout="@layout/fragment_balance_detail" />
</navigation>